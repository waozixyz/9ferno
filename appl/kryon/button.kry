

@limbo
handleClickLimbo: fn() {
    sys->print("Hello from Limbo+Kryon!\n");
};
@end

Window {
  title = "Interactive Button Demo"
  width = 600
  height = 400
  background = "#191919"

  Center {
    Button {
      text = "Click Me!"
      width = 150
      height = 50
      color = "pink"
      background = "#404080"
      onClick = handleClickLimbo
    }
  }
}
